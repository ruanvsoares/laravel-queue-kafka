{
  "name": "ruanvsoares/laravel-queue-kafka",
  "description": "Kafka driver for Laravel Queue",
  "license": "MIT",
  "type": "library",
  "version": "1.1",
  "authors": [
    {
      "name": "Peter Mein",
      "email": "petermein@gmail.com"
    },
    {
      "name": "Ruan Soares",
      "email": "ruansvictor@gmail.com"
    }
  ],
  "require": {
    "php": "8.*",
    "illuminate/database": "11.* ",
    "illuminate/support": "11.*",
    "illuminate/queue": "11.*",
    "ext-rdkafka": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "11.*",
    "mockery/mockery": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Rapide\\LaravelQueueKafka\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rapide\\LaravelQueueKafka\\LaravelQueueKafkaServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
