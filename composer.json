{
  "name": "ruanvsoares/laravel-queue-kafka",
  "description": "Kafka driver for Laravel Queue",
  "license": "MIT",
  "type": "library",
  "version": "1.0",
  "authors": [
    {
      "name": "Peter Mein",
      "email": "petermein@gmail.com"
    }
  ],
  "require": {
    "php": "8.*",
    "illuminate/database": "8.* || 9.* || 10.* || 11.* ",
    "illuminate/support": "8.* || 9.* || 10.* || 11.*",
    "illuminate/queue": "8.* || 9.* || 10.* || 11.*",
    "ext-rdkafka": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "10.*",
    "mockery/mockery": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Rapide\\LaravelQueueKafka\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rapide\\LaravelQueueKafka\\LaravelQueueKafkaServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
